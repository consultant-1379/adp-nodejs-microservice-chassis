{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "test": "web-test-runner --playwright --browsers firefox chromium",
    "test:install": "playwright install firefox chromium webkit",
    "test:all": "web-test-runner --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner --watch --playwright --browsers chromium",
    "start": "snowpack dev",
    "build:dev": "snowpack build",
    "build:prod": "snowpack build && rollup -c",
    "srv": "node server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .eslintignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .eslintignore",
    "lint:prettier": "prettier \"**/*.{js,html}\" --check --ignore-path .eslintignore",
    "format:prettier": "prettier \"**/*.{js,html}\" --write --ignore-path .eslintignore",
    "test:automation:chrome": "playwright test --project=chromium",
    "test:automation:firefox": "playwright test --project=firefox",
    "test:automation:chrome:dev": "playwright test --debug --project=chromium",
    "test:automation:firefox:dev": "playwright test --debug --project=firefox",
    "test:automation:chrome:headed": "playwright test --headed --project=chromium",
    "test:automation:firefox:headed": "playwright test --headed --project=firefox",
    "test:automation:all": "playwright test",
    "test:automation:report": "playwright show-report",
    "e2e:local": "wdio test_js/config/wdio.conf.js --selenium-standalone --local-mockserver --spec-reporter",
    "e2e:local-docker": "wdio test_js/config/wdio.conf.js --selenium-hub --local-mockserver --spec-reporter",
    "e2e:CI": "wdio test_js/config/wdio.conf.js --ci --selenium-hub --network-config-from-env  --spec-reporter --allure-reporter",
    "e2e:generateReport": "node scripts/test_js-generate-report.js",
    "openReport": "allure open test_js/allure-report",
    "sonar-scanner": "sonar-scanner"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eui/app": "^1.0.0",
    "@eui/container": "^1.1.0",
    "@eui/lit-component": "^1.0.0",
    "@eui/navigation-menu": "^1.1.3",
    "@eui/theme": "^1.1.2",
    "@webcomponents/scoped-custom-element-registry": "^0.0.5",
    "es-module-shims": "1.5.18",
    "urlpattern-polyfill": "5.0.9"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/eslint-parser": "7.21.3",
    "@eui/import-css-plugin": "1.0.0",
    "@open-wc/eslint-config": "10.0.0",
    "@open-wc/testing": "3.1.7",
    "@open-wc/testing-helpers": "2.2.0",
    "@playwright/test": "1.36.2",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@snowpack/web-test-runner-plugin": "0.2.2",
    "@wdio/allure-reporter": "^8.12.3",
    "@wdio/cli": "^8.13.13",
    "@wdio/local-runner": "^8.13.13",
    "@wdio/mocha-framework": "^8.13.13",
    "@wdio/selenium-standalone-service": "^8.13.13",
    "@wdio/spec-reporter": "^8.12.2",
    "@web/dev-server-import-maps": "0.0.8",
    "@web/test-runner": "0.15.1",
    "@web/test-runner-junit-reporter": "0.5.0",
    "@web/test-runner-playwright": "0.10.1",
    "allure-commandline": "^2.23.1",
    "chai": "^4.3.7",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-lit-a11y": "2.4.0",
    "express": "4.18.2",
    "glob": "8.0.3",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "2.8.6",
    "rollup": "2.79.1",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "rollup-plugin-summary": "1.4.3",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "15.0.2",
    "snowpack": "3.8.8",
    "sonarqube-scanner": "^2.8.1"
  }
}
